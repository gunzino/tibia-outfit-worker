/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "outfit-worker",
	"main": "src/index.js",
	"compatibility_date": "2026-02-12",
	"observability": {
		"enabled": false
	},
	"compatibility_flags": [
		"nodejs_compat"
	],
	"assets": {
		"directory": "./assets/",
		"binding": "ASSETS",
		"run_worker_first": ["/*"]
	}
}
